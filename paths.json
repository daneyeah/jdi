{
  "public": {
    "clean": "./public/",
    "admin":{
      "css": "public/admin/assets/css/",
      "js": "public/admin/assets/js/",
      "js_lib": "public/admin/assets/js/lib/",
      "primary": "public/admin/",
      "image": "public/admin/assets/images",
      "font": "public/admin/assets/fonts/"
    },
    "desktop":{
      "css": "public/desktop/assets/css/",
      "js": "public/desktop/assets/js/",
      "js_lib": "public/desktop/assets/js/lib/",
      "primary": "public/desktop/",
      "image": "public/desktop/assets/images",
      "font": "public/desktop/assets/fonts/"
    },
    "mobile":{
      "css": "public/mobile/assets/css/",
      "js": "public/mobile/assets/js/",
      "js_lib": "public/mobile/assets/js/lib/",
      "primary": "public/mobile/",
      "image": "public/mobile/assets/images",
      "font": "public/mobile/assets/fonts/"
    }
  },
  "src": {
    "admin": {
      "css": "sources/admin/assets/css/*.css",
      "min_css": "sources/admin/assets/css/min/*.css",
      "image": "sources/admin/assets/images/**/*.{png,jpg,jpeg}",
      "less": "sources/admin/assets/less/**/*.less",
      "ignore_less": "!sources/admin/assets/less/**/__*.less",
      "js": "sources/admin/assets/js/*.js",
      "json": "sources/admin/**/*.json",
      "php": "sources/admin/**/*.php",
      "pug": "sources/admin/**/*.pug"
    },
    "desktop": {
      "css": "sources/desktop/assets/css/*.css",
      "min_css": "sources/desktop/assets/css/min/*.css",
      "image": "sources/desktop/assets/images/**/*.{png,jpg,jpeg}",
      "less": "sources/desktop/assets/less/**/*.less",
      "ignore_less": "!sources/desktop/assets/less/**/__*.less",
      "js": "sources/desktop/assets/js/*.js",
      "pug": "sources/desktop/**/*.pug",
      "php": "sources/desktop/**/*.php",
      "json": "sources/desktop/**/*.json"
    },
    "common": {
      "css": "sources/common/assets/css/*.css",
      "min_css": "sources/common/assets/css/min/*.css",
      "less": "sources/common/assets/less/**/*.less",
      "ignore_less": "!sources/common/assets/less/**/__*.less",
      "image": "sources/common/assets/images/**/*.{png,jpg,jpeg}",
      "htaccess": "sources/common/.htaccess",
      "font": {
        "less": "sources/common/assets/fonts/less/*.less",
        "ignore_less": "!sources/common/assets/fonts/less/__*.less",
        "js": "sources/common/assets/fonts/js/*.js",
        "css": "sources/common/assets/fonts/css/*.css",
        "file": "sources/common/assets/fonts/*.{eot,woff,svg,ttf}"
      },
      "js": "sources/common/assets/js/*.js",
      "pug": "sources/common/**/*.pug",
      "php": "sources/common/**/*.php",
      "json": "sources/common/**/*.json"
    },
    "mobile": {
      "css": "sources/mobile/assets/css/*.css",
      "min_css": "sources/mobile/assets/css/min/*.css",
      "image": "sources/mobile/assets/images/**/*.{png,jpg,jpeg}",
      "less": "sources/mobile/assets/less/**/*.less",
      "ignore_less": "!sources/mobile/assets/less/**/__*.less",
      "js": "sources/mobile/assets/js/*.js",
      "pug": "sources/mobile/**/*.pug",
      "php": "sources/mobile/**/*.php",
      "json": "sources/mobile/**/*.json"
    },
    "library": {
      "jquery": "./node_modules/jquery/dist/jquery.min.js"
    }
  },
  "dist":{
    "clean": "./storage/",
    "admin": {
      "css": "sources/admin/assets/css",
      "min_css": "sources/admin/assets/css/min"
    },
    "desktop": {
      "css": "sources/desktop/assets/css",
      "min_css": "sources/desktop/assets/css/min"
    },
    "common": {
      "css": "sources/common/assets/css",
      "min_css": "sources/common/assets/css/min",
      "font": {
        "css": "sources/common/assets/fonts/css"
      }
    },
    "mobile": {
      "css": "sources/mobile/assets/css",
      "min_css": "sources/mobile/assets/css/min"
    }
  }
}